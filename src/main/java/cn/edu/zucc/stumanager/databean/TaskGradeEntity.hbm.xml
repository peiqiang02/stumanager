<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.zucc.stumanager.databean.TaskGradeEntity" table="task_grade" schema="stu_grade_manager">
        <id name="id" column="id"/>
        <property name="taskId" column="task_id"/>
        <property name="curId" column="cur_id"/>
        <property name="stuId" column="stu_id"/>
        <property name="taskScore" column="task_score"/>
    </class>
</hibernate-mapping>