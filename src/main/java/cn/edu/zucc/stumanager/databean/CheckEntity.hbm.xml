<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.zucc.stumanager.databean.CheckEntity" table="check" schema="stu_grade_manager">
        <id name="id" column="id"/>
        <property name="curId" column="cur_id"/>
        <property name="checkDate" column="check_date"/>
        <property name="shouldNum" column="shouldNum"/>
        <property name="lateNum" column="lateNum"/>
        <property name="notNum" column="notNum"/>
    </class>
</hibernate-mapping>