<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.zucc.stumanager.databean.StudentEntity" table="student" schema="stu_grade_manager">
        <id name="stuId" column="stu_id"/>
        <property name="stuAge" column="stu_age"/>
        <property name="stuName" column="stu_name"/>
        <property name="stuSex" column="stu_sex"/>
        <property name="stuMajor" column="stu_major"/>
        <property name="stuClass" column="stu_class"/>
    </class>
</hibernate-mapping>